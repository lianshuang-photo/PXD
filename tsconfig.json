{
  "compilerOptions": {
    "target": "ES2020",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "lib": ["DOM", "DOM.Iterable", "ES2021"],
    "skipLibCheck": true,
    "moduleResolution": "Bundler",
    "allowJs": false,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "noImplicitAny": false,
    "jsx": "react-jsx",
    "types": ["uxp"]
  },
  "include": ["src"],
  "references": []
}
